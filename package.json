{
  "name": "mevo-test",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "nodemon --ext js,ts,json,env --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "prepare": "husky install",
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf tsconfig.build.tsbuildinfo && rm -rf ./dist",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm run test -- --colors --coverage --ci",
    "lint": "eslint --ext .ts,.js .",
    "format": "prettier \"./**\" --write --ignore-unknown",
    "format:check": "prettier \"./**\" --ignore-unknown --check"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.19.2",
    "fast-csv": "^5.0.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.3",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.6",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jest/globals": "^28.0.0",
    "@jest/types": "^28.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.19.26",
    "@types/pg": "^8.11.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
